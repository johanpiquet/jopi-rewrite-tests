{
  "name": "jopi-rewrite-test",
  "version": "1.0.0",
  "private": "true",
  "engines": {
    "bun": "^1.2.18",
    "node": ">=22.18.0"
  },
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "tsc": "tsc",
    "bun-install": "bun install --registry http://localhost:4873/",
    "bun-update": "bun update --registry http://localhost:4873/",
