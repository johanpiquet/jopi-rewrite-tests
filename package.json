    "start-bun": "bun --preload jopi-loader src/tests.ts"
  },
  "devDependencies": {
    "@types/bun": "^1.2.20",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "typescript": "^5.9.2",
    "jopi-loader": "latest"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "jopi-rewrite": "latest",
    "jopi-rewrite-ui": "latest",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
